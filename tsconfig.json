{
    "compilerOptions": {
      "baseUrl": ".",
      "esModuleInterop": true,
      "target": "ES2022",
      "lib": [
        "DOM",
        "DOM.Iterable",
        "ES2022"
      ],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": false,
      "strictNullChecks": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "incremental": true,
      "jsx": "preserve",
      "module": "esnext",
      "moduleResolution": "bundler",
      "resolveJsonModule": true,
      "sourceMap": true,
      "isolatedModules": true,
      "plugins": [
        {
          "name": "next"
        }
      ],
      "paths": {
        "react": [
          "./node_modules/@types/react"
        ],
        "@/*": [
          "./src/*"
        ],
      }
    },
    "include": [
      "**/*.ts",
    ],
    "exclude": [
      "node_modules"
    ]
  }
  